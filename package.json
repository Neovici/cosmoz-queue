{
  "name": "@neovici/cosmoz-queue",
  "version": "1.0.0",
  "description": "A reusable queue component for master-detail views with list, split, and queue modes",
  "keywords": [
    "web-components",
    "queue",
    "master-detail",
    "split-view",
    "pion",
    "lit-html"
  ],
  "homepage": "https://github.com/neovici/cosmoz-queue#readme",
  "bugs": {
    "url": "https://github.com/neovici/cosmoz-queue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neovici/cosmoz-queue.git"
  },
  "license": "Apache-2.0",
  "author": "",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "tsc && eslint --cache .",
    "build": "tsc -p tsconfig.build.json",
    "start": "wds",
    "test": "wtr --coverage",
    "test:watch": "wtr --watch",
    "dev": "npm run storybook:start",
    "storybook:start": "storybook dev -p 8000",
    "storybook:build": "storybook build",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:preview": "npm run storybook:build && http-server -d ./storybook-static/",
    "prepare": "husky"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "branch": "master",
    "preset": "conventionalcommits"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "types"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./hooks/use-queue": "./dist/hooks/use-queue.js",
    "./hooks/use-tabs": "./dist/hooks/use-tabs.js",
    "./hooks/use-split": "./dist/hooks/use-split.js",
    "./hooks/use-data-nav": "./dist/hooks/use-data-nav.js",
    "./hooks/use-list-state": "./dist/hooks/use-list-state.js",
    "./render": "./dist/render.js"
  },
  "dependencies": {
    "@neovici/cosmoz-i18next": "^3.5.1",
    "@neovici/cosmoz-omnitable": "^16.0.3",
    "@neovici/cosmoz-router": "^11.2.8",
    "@neovici/cosmoz-slider": "^5.2.1",
    "@neovici/cosmoz-tabs": "^9.3.2",
    "@neovici/cosmoz-utils": "^6.14.2",
    "@neovici/cosmoz-viewinfo": "^4.1.1",
    "@pionjs/pion": "^2.5.2",
    "i18next": "^25.7.4",
    "lit-html": "^3.3.1",
    "split.js": "^1.6.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@eslint/eslintrc": "^2.0.0",
    "@neovici/cfg": "^2.0.0",
    "@neovici/testing": "^2.0.0",
    "@open-wc/testing": "^4.0.0",
    "@open-wc/testing-helpers": "^3.0.1",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/web-components": "7.6.17",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.10.2",
    "@types/split.js": "^1.6.0",
    "@web/dev-server-esbuild": "^1.0.4",
    "@web/storybook-builder": "^0.1.9",
    "@web/storybook-framework-web-components": "^0.1.1",
    "@web/test-runner-playwright": "^0.11.1",
    "esbuild": "^0.24.0",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "rollup-plugin-esbuild": "^6.1.1",
    "semantic-release": "^24.0.0",
    "sinon": "^19.0.0",
    "storybook": "^7.0.0",
    "typescript": "^5.4.3"
  },
  "overrides": {
    "conventional-changelog-conventionalcommits": ">= 8.0.0",
    "@polymer/polymer": "^3.5.2"
  }
}
